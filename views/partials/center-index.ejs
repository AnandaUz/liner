        <div class="articles-list">
            <% if (articles && articles.length > 0) { %>
                <%
                    let m = articles.filter( article => article.isTechnique)


                m.forEach(article => { %>
                    <a href="/texts/<%= article.slug %>">
                        <%= article.title %>
                    </a>
                <% }); %>
            <% } else { %>
                <p>Статей пока нет.</p>
            <% } %>
        </div>
        <h3>СТАТЬИ</h3>
        <div class="articles-list">
            <%
            let m = articles.filter( article => article.isArrticle)
            if (m && m.length > 0) { %>
